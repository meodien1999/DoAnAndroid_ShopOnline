@using WebAppDongHo.Models;
@model List<KhachHangOnline>
@{
    ViewBag.Title = "KhachHang";
    Layout = "~/Views/Admin/_LayoutAdmin.cshtml";
}
<link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
<script src="~/Content/bootstrap/js/bootstrap.min.js"></script>

<h4><b><i class="fas fa-book"></i>Thông tin Khách Hàng Online</b></h4>
<table class="table table-bordered">
    <tr class="thead-light">
        <th>Mã Khách hàng</th>
        <th>Email</th>
        <th>Mật khẩu</th>
        <th>Họ tên</th>
        <th>Địa chỉ</th>
        <th>Số điện thoại</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.MaKH</td>
            <td>@item.Email</td>
            <td>@item.MatKhau</td>
            <td>@item.HoTen</td>
            <td>@item.DiaChi</td>
            <td>@item.SDT</td>
        </tr>
    }
</table>